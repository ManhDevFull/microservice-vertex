  - kind: 2
    languageId: sql
    value: "select distinct p.* from product p\r

      \            JOIN category c ON \r

      \            (p.category = c.id)\r

      \            JOIN variant v on (p.id = v.product_id)\r

      \r

      SELECT COUNT(*) \r

      FROM orders o\r

      JOIN variant v ON o.variant_id = v.id\r

      WHERE v.product_id = 1;\r

      \r

      \r

      SELECT  sum(r.rating) from review r\r

      join orders o on (r.order_id = o.id)\r

      join variant v on (v.id = o.variant_id)\r

      WHERE (v.product_id = 1)\r

      \r

      \r

      \r

      select * from review\r

      select * from orders\r

      select * from  variant WHERE product_id = 1\r

      \r

      \r

      SELECT  r.id as idreview, o.id as idorder, \r

      v.product_id as idproduct, r.rating, v.id as idvariant, v.product_id as
      idporduct from review r\r

      join orders o on (r.order_id = o.id)\r

      join variant v on (v.id = o.variant_id)\r

      WHERE (v.product_id = 2)\r

      \r

      select DISTINCT * from product p join variant ON (p.id =
      variant.product_id)\r

      WHERE variant.price between 25000 and 30000\r

      \r

      \r

      \r

      SELECT\r

      \    v.product_id,\r

      \    v.valuevariant ->> 'price' AS price_value\r

      FROM\r

      \    variant v\r

      WHERE\r

      \    v.valuevariant ? 'price';\r

      \r

      \r

      \r

      select top 10 * FROM product p\r

      join category c ON (p.category = c.id)\r

      join variant v ON (p.id = v.product_id)\r

      JOIN orders o ON (v.id = orders.variant_id)\r\n"
